<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Pojos.Horario" table="horarios">
  <id column="idhorario" name="idhorario" type="java.lang.Integer">
    <generator class="increment"/>
  </id>
  <property name="idreunion" column="idreunion" type="java.lang.Integer"/>
  <property name="fechainicio" column="fechainicio" type="java.util.Date"/>
  <property name="fechafin" column="fechafin" type="java.util.Date"/>
  </class>
</hibernate-mapping>
